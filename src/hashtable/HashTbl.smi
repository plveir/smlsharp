_require "../basis.smi"
_require "../reify.smi"

structure HashTbl =
struct
  type 'v bucket (= list)

  type hashtbl (= boxed)

  val create  : {hash: string -> word, eq: string * string -> bool} -> hashtbl
  val default : unit -> hashtbl
  val size    : hashtbl -> int
  val add     : hashtbl -> string -> Dynamic.void Dynamic.dyn -> hashtbl
  val find    : hashtbl -> string -> Dynamic.void Dynamic.dyn
  (* val find    : ['a#reify. hashtbl -> string -> 'a] *)
end